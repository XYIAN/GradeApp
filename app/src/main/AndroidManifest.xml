<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kxdilbeck.gradeapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".View.CreateAccountActivity"></activity>
        <activity android:name=".CoursePage"></activity>
        <activity android:name=".View.AssignmentDashboardActivity"></activity>
        <activity android:name=".View.CreateAssignmentActivity"></activity>
        <activity android:name=".View.CreateCourse"></activity>
    </application>

</manifest>